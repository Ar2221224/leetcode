// The rand7() API is already defined for you.
// int rand7();
// @return a random integer in the range 1 to 7

class Solution {
public:
    int rand10() {
        int first=7,second=4;
        while (first>5) first=rand7();
        while (second==4) second=rand7();
        return first+(second>4?5:0);
    }
};
